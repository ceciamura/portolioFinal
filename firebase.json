{
  "hosting": {
    "public": "./dist/portolio-arg-prog",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers":[
      {
        "source:": "https://portfolio-ceci-amura.herokuapp.com",
        "headers": [{
          "key": "Access-Control-Allow-Origin"
        }]
      }
    ]
  }
}
